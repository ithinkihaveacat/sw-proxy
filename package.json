{
  "name": "sw-proxy",
  "version": "0.2.0",
  "description": "A RFC-compliant HTTP proxy library for service workers.",
  "keywords": [
    "service-worker",
    "serviceworker",
    "proxy",
    "http",
    "rfc7234",
    "rfc5861"
  ],
  "homepage": "https://github.com/ithinkihaveacat/sw-proxy",
  "license": "Apache-2.0",
  "author": "Michael Stillwell <mjs@beebo.org> (https://beebo.org/)",
  "files": [
    "dist",
    "service-worker.d.ts"
  ],
  "repository": "ithinkihaveacat/sw-proxy",
  "main": "dist/commonjs/proxy.js",
  "types": "dist/commonjs/proxy.d.ts",
  "jsnext:main": "dist/es6/proxy.js",
  "devDependencies": {
    "eslint": "^3.13.1",
    "eslint-config-google": "^0.7.1",
    "mocha": "^3.2.0",
    "node-mocks-http": "^1.5.6",
    "rollup": "^0.41.4",
    "rollup-plugin-typescript": "^0.8.1",
    "tslint": "^4.3.1",
    "typescript": "^2.1.1"
  },
  "scripts": {
    "preversion": "$npm_execpath test",
    "prepare": "tsc -p tsconfig.json --module commonjs --outDir dist/commonjs && touch dist/commonjs/service-worker.js && tsc -p tsconfig.json --module es6 --outDir dist/es6 && touch dist/es6/service-worker.js",
    "postpublish": "git push && git push --tags",
    "test": "$npm_execpath run prepare && $npm_execpath run lint && mocha",
    "test:watch": "$npm_execpath run test -- -w",
    "lint": "eslint test/*.js && tslint router.ts proxy.ts",
    "mocha": "mocha",
    "printenv": "printenv",
    "examples": "cd examples ; for f in *.ts ; do rollup -c -i $f -o `basename $f .ts`.js ; done"
  }
}
